#include <iostream>
using namespace std;

int main() {
    int a = 5;              // Declare an integer variable and initialize it to 5
    float b = 5.5;          // Declare a float variable and initialize it to 5.5
    char c = 'A';           // Declare a char variable and initialize it to 'A'
    string d = "Hello";     // Declare a string variable and initialize it to "Hello"
    bool e = true;          // Declare a bool variable and initialize it to true

    cout << "a = " << a << endl;
    cout << "b = " << b << endl;
    cout << "c = " << c << endl;
    cout << "d = " << d << endl;
    cout << "e = " << e << endl;

    return 0;
}

/*
Output:
a = 5
b = 5.5
c = A
d = Hello
e = 1
*/
